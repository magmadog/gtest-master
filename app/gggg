void po(text txt,node* pp, char *lok){
	node* tmp;
	tmp = malloc(sizeof(node));
	node* next;
	next = pp->next;
        if(next != NULL){
                next->previous = tmp;
        }
	tmp->next = next;
	tmp->previous = pp;
	pp->next = tmp;
	if(next == NULL)
	{
		txt->end = tmp;
		//txt->end->next = NULL;
	}
	int i;
	for( i = 0 ; lok[i] != '0' ; i++)
		tmp->contents[i] = lok[i];
	txt->length++;
}
